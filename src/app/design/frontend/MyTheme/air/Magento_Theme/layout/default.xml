<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
<!-- remove element-->
        <referenceBlock name="catalog.compare.link" remove="true" />
        <referenceBlock name="catalog.compare.sidebar" remove="true"/>
        <referenceBlock name="catalogsearch.product.addto.compare" remove="true"/>
        <referenceBlock name="category.product.addto.compare" remove="true"/>
        <referenceBlock name="crosssell.product.addto.compare" remove="true" />
        <referenceBlock name="related.product.addto.compare" remove="true" />
        <referenceBlock name="upsell.product.addto.compare" remove="true" />
        <referenceBlock name="view.addto.compare" remove="true" />
        <referenceBlock name="header" remove="true"/>
<!-- remove element-->
        <referenceContainer name="header.container" >
            <container name="wrapper_block" htmlClass="wrapper_block" htmlTag="div">
<!--                <block name="test-js" template="Magento_Theme::test-js.phtml"></block>-->
            </container>
        </referenceContainer>
        <referenceBlock name="header.links">
            <block class="Magento\Framework\View\Element\Html\Link" name="test_li" before="-">
                <arguments>
                    <argument name="label" xsi:type="string">Extended 6 Month Free Returns</argument>
                    <argument name="title" xsi:type="string">Extended 6 Month Free Returns</argument>
                    <argument name="href" xsi:type="string">delivery-information</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="test" template="Magento_Theme::test.phtml">
                <arguments>
                    <argument name="test_data" xsi:type="string">test data</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Template" name="test-link">
                <arguments>
                    <argument name="title" xsi:type="string">test title header links</argument>
                    <argument name="path" xsi:type="string">/contact</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
